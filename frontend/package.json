{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test": "jest",
    "test:ci:unit": "jest --coverage --testPathIgnorePatterns=__tests__ end_to_end",
    "test:ci:integration": "jest --coverage --testPathPattern=__tests__ --testPathIgnorePatterns=end_to_end",
    "cucumber": "cucumber-js",
    "end_to_end": "playwright test",
    "end_to_end:headed": "playwright test --headed",
    "end_to_end:report": "playwright show-report",
    "generate-docs": "npx jsdoc -c jsdoc.json"
  },
  "dependencies": {
    "@clerk/nextjs": "^5.7.5",
    "@clerk/themes": "^2.1.37",
    "chart.js": "^4.4.4",
    "chartjs-plugin-datalabels": "^2.2.0",
    "next": "14.2.13",
    "react": "^18",
    "react-chartjs-2": "^5.2.0",
    "react-dom": "^18",
    "react-icons": "^5.3.0",
    "taffydb": "^2.7.3"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.25.7",
    "@babel/preset-react": "^7.25.7",
    "@cucumber/cucumber": "^11.0.1",
    "@playwright/test": "^1.47.2",
    "babel-jest": "^29.7.0",
    "coveralls": "^3.1.1",
    "docdash": "^2.0.2",
    "eslint": "^8",
    "eslint-config-next": "14.2.13",
    "jest": "^29.7.0",
    "jsdoc": "^4.0.3",
    "minami": "^1.2.3",
    "react-test-renderer": "^18.3.1",
    "supertest": "^7.0.0"
  }
}
